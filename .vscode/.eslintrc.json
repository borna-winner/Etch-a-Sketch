{
    "browser": true,
    "env": {
        "es2021": true
    },
    "extends": [
        "airbnb/base",
        "eslint-config-prettier"
      ],
      "plugins": [
        "eslint-plugin-prettier"
      ],

    "rules": {
        "prettier/prettier": "error",
        "no-unused-vars": "warn",
        "no_console": "off"
    }
}
